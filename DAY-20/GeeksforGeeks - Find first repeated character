class Solution 
{ 
    String firstRepChar(String s) 
    { 
       HashSet<Character> hash=new HashSet<>();
        char ch[]=s.toCharArray();
        for(int i=0;i<ch.length;i++)
        {
            if(hash.contains(arr[i]))
             {   
                return String.value(arr[i]);
             }
            else
                hash.add(arr[i]);
        }
        return null;
    }
} 
