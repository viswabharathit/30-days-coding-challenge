class Solution {
    public int lengthOfLastWord(String s) {
        String[] array=s.split(" ");
        int n=array.length;
        int m=0;
        for(int i=0;i<n;i++)
        {
            if(i==n-1)
            {
                m=array[i].length();
            }
        }
        return m;
    }
}
